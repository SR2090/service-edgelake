{
    "org": "anylog",
    "label": "anylog-node for amd64",
    "description": "A sample AnyLog service to deploy a Master Node",
    "documentation": "https://github.com/open-horizon-services/service-anylog/tree/os-dev/README.md",
    "url": "anylog-node",
    "version": "1.3",
    "arch": "amd64",
    "sharable": "multiple",
    "requiredServices": [],
    "userInput": [
        {
            "name": "INIT_TYPE",
            "defaultValue": "training"
        },
        {
            "name": "ANYLOG_PATH",
            "defaultValue": "/app"
        },
        {
            "name": "LOCAL_SCRIPTS",
            "defaultValue": "/app/deployment-scripts/training-deployment"
        },
        {
            "name": "TEST_DIR",
            "defaultValue": "/app/deployment-scripts/test"
        },
        {
            "name": "LICENSE_KEY",
            "defaultValue": "0f6068c4c868bc1bb2caace57895c091c2d659f4af44ad275b260e93d1b556b48a3c0b18674b57e6f0ee795ec7f4dcda0957bae8c17ecb1d9006b6d8a44219494bc6cce29f01c0538005afb674f44fdff2ded4e0e9b3ff2b21afc35b540a142f36c9d2e9f6c3549c08476bf5102f01bc997d00f1a4bd8b4c73d212c3f06f5cfa2023-11-25bIBM"
        },
        {
            "name": "NODE_TYPE",
            "defaultValue": "operator"
        },
        {
            "name": "NODE_NAME",
            "defaultValue": "anylog-operator2"
        },
        {
            "name": "COMPANY_NAME",
            "defaultValue": "Open Horizon"
        },
        {
            "name": "ANYLOG_SERVER_PORT",
            "defaultValue": "32148"
        },
        {
            "name": "ANYLOG_REST_PORT",
            "defaultValue": "32149"
        },
        {
            "name": "CLUSTER_NAME",
            "defaultValue": "open-horizon-cluster2"
        },
        {
            "name": "DEFAULT_DBMS",
            "defaultValue": "open_horizon"
        },
        {
            "name": "LEDGER_CONN",
            "defaultValue": "172.105.4.104:32048"
        },
        {
            "name": "ENABLE_MQTT",
            "defaultValue": "true"
        },
        {
            "name": "MONITOR_NODES",
            "defaultValue": "true"
        }
    ],
    "deployment": {
        "services": {
            "anylog-node": {
                "image": "anylogco/anylog-network:beta",
                "network": "host"
            }
        }
    }
}
