{
    "org": "anylog",
    "label": "anylog-node for amd64",
    "description": "A sample AnyLog service to deploy a Master Node",
    "documentation": "https://github.com/open-horizon-services/service-anylog/tree/os-dev/README.md",
    "url": "anylog-node",
    "version": "1.3",
    "arch": "amd64",
    "sharable": "multiple",
    "requiredServices": [],
    "userInput": [
        {
            "name": "INIT_TYPE",
            "defaultValue": "prods"
        },
        {
            "name": "ANYLOG_PATH",
            "defaultValue": "/app"
        },
        {
            "name": "LOCAL_SCRIPTS",
            "defaultValue": "/app/deployment-scripts/training-deployment"
        },
        {
            "name": "TEST_DIR",
            "defaultValue": "/app/deployment-scripts/test"
        },
        {
            "name": "LICENSE_KEY",
            "defaultValue": "7d6a0136fa2722022431c8ca40c194f725d3a989dcec7ef9ddfeccca83cadf05776f6ef86c7f7e3cbb2b5283e7000a1e37cec998648036e069a24ff7a9c6de70e6472d9a7dc9cf8fb8a005e98e0e212b8402d4fb185f319cf9c75d20daee373b47edd4d86d6c9698dd56e96d848e952ad8ac6d39251370e445f11ea651aab6fe{'company':'Ori','expiration':'2024-07-12','type':'beta'}"
        },
        {
            "name": "NODE_TYPE",
            "defaultValue": "master"
        },
        {
            "name": "NODE_NAME",
            "defaultValue": "anylog-master"
        },
        {
            "name": "COMPANY_NAME",
            "defaultValue": "Open Horizon"
        },
        {
            "name": "ANYLOG_SERVER_PORT",
            "defaultValue": "32048"
        },
        {
            "name": "ANYLOG_REST_PORT",
            "defaultValue": "32049"
        },
        {
            "name": "LEDGER_CONN",
            "defaultValue": "127.0.0.1:32048"
        },
        {
            "name": "MONITOR_NODES",
            "defaultValue": "false"
        }
    ],
    "deployment": {
        "services": {
            "anylog-node": {
                "image": "anylogco/anylog-network:edgelake",
                "network": "host"
            }
        }
    }
}
