{
    "org": "anylog",
    "label": "anylog-node for amd64",
    "description": "A sample AnyLog service to deploy a Master Node",
    "documentation": "https://github.com/open-horizon-services/service-anylog/tree/os-dev/README.md",
    "url": "anylog-training-node",
    "version": "1.3.2310",
    "arch": "amd64",
    "sharable": "multiple",
    "requiredServices": [],
    "userInput": [
        {
            "name": "INIT_TYPE",
            "defaultValue": "prod"
        },
        {
            "name": "ANYLOG_PATH",
            "defaultValue": "/app"
        },
        {
            "name": "LOCAL_SCRIPTS",
            "defaultValue": "/app/deployment-scripts/node-deployment"
        },
        {
            "name": "TEST_DIR",
            "defaultValue": "/app/deployment-scripts/test"
        },
        {
            "name": "LICENSE_KEY",
            "defaultValue": ""
        },
        {
            "name": "NODE_TYPE",
            "defaultValue": "operator"
        },
        {
            "name": "NODE_NAME",
            "defaultValue": "anylog-operator"
        },
        {
            "name": "COMPANY_NAME",
            "defaultValue": "New Company"
        },
        {
            "name": "ANYLOG_SERVER_PORT",
            "defaultValue": "32148"
        },
        {
            "name": "ANYLOG_REST_PORT",
            "defaultValue": "32149"
        },
        {
            "name": "ANYLOG_BROKER_PORT",
            "defaultValue": ""
        },
        {
            "name": "TCP_BIND",
            "defaultValue": "false"
        },
        {
            "name": "REST_BIND",
            "defaultValue": "false"
        },
        {
            "name": "BROKER_BIND",
            "defaultValue": "false"
        },
        {
            "name": "DB_TYPE",
            "defaultValue": "sqlite"
        },
        {
            "name": "DB_USER",
            "defaultValue": ""
        },
        {
            "name": "DB_PASSWD",
            "defaultValue": ""
        },
        {
            "name": "DB_IP",
            "defaultValue": "127.0.0.1"
        },
        {
            "name": "DB_PORT",
            "defaultValue": "5432"
        },
        {
            "name": "LEDGER_CONN",
            "defaultValue": "127.0.0.1:32048"
        },
        {
            "name": "CLUSTER_NAME",
            "defaultValue": "new-company-cluster1"
        },
        {
            "name": "DEFAULT_DBMS",
            "defaultValue": "new_company"
        },
        {
            "name": "ENABLE_PARTITIONS",
            "defaultValue": "true"
        },
        {
            "name": "PARTITION_COLUMN",
            "defaultValue": "insert_timestamp"
        },
        {
            "name": "PARTITION_INTERVAL",
            "defaultValue": "1 day"
        },
        {
            "name": "PARTITION_KEEP",
            "defaultValue": "3"
        },
        {
            "name": "ENABLE_MQTT",
            "defaultValue": "false"
        },
        {
            "name": "MONITOR_NODES",
            "defaultValue": "true"
        }
    ],
    "deployment": {
        "services": {
            "anylog-node": {
                "image": "anylogco/anylog-network:latest",
                "network": "host"
            }
        }
    }
}
