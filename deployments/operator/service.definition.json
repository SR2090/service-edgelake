{
    "org": "anylog",
    "label": "anylog-node for amd64",
    "description": "A sample AnyLog service to deploy a Master Node",
    "documentation": "https://github.com/open-horizon-services/service-anylog/tree/os-dev/README.md",
    "url": "anylog-training-node",
    "version": "1.3.2310",
    "arch": "amd64",
    "sharable": "multiple",
    "requiredServices": [],
    "userInput": [
        {
            "name": "INIT_TYPE",
            "defaultValue": "training"
        },
        {
            "name": "ANYLOG_PATH",
            "defaultValue": "/app"
        },
        {
            "name": "LOCAL_SCRIPTS",
            "defaultValue": "/app/deployment-scripts/training-deployment"
        },
        {
            "name": "TEST_DIR",
            "defaultValue": "/app/deployment-scripts/test"
        },
        {
            "name": "LICENSE_KEY",
            "defaultValue": "b7133573e398615da8fb419aedfa51af90307c9c3b1a2278561cf7e1040ade3b4a40cc8263724f9128de96198c13a447b1bd49b972f7e7bb37e6d3152d8ef139ed18514e6aa2038793e80068f586c12aee70718f43a5bfa901bf94844bf1a94087155f83c49980775518ace2de986af25ee2c5e5a0efc2833d16078aad9e28642024-02-01bGuest"
        },
        {
            "name": "NODE_TYPE",
            "defaultValue": "operator"
        },
        {
            "name": "NODE_NAME",
            "defaultValue": "anylog-operator2"
        },
        {
            "name": "COMPANY_NAME",
            "defaultValue": "AnyLog"
        },
        {
            "name": "ANYLOG_SERVER_PORT",
            "defaultValue": "32148"
        },
        {
            "name": "ANYLOG_REST_PORT",
            "defaultValue": "32149"
        },
        {
            "name": "CLUSTER_NAME",
            "defaultValue": "anylog-cluster1"
        },
        {
            "name": "DEFAULT_DBMS",
            "defaultValue": "anylog"
        },
        {
            "name": "LEDGER_CONN",
            "defaultValue": "132.177.125.232:32048"
        },
        {
            "name": "ENABLE_MQTT",
            "defaultValue": "true"
        },
        {
            "name": "MONITOR_NODES",
            "defaultValue": "true"
        }
    ],
    "deployment": {
        "services": {
            "anylog-node": {
                "image": "anylogco/anylog-network:training",
                "network": "host"
            }
        }
    }
}
