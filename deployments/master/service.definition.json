{
    "org": "anylog",
    "label": "anylog-node for amd64",
    "description": "A sample AnyLog service to deploy a Master Node",
    "documentation": "https://github.com/open-horizon-services/service-anylog/tree/os-dev/README.md",
    "url": "anylog-node",
    "version": "1.3",
    "arch": "amd64",
    "sharable": "multiple",
    "requiredServices": [],
    "userInput": [
        {
            "name": "INIT_TYPE",
            "defaultValue": "prod"
        },
        {
            "name": "ANYLOG_PATH",
            "defaultValue": "/app"
        },
        {
            "name": "LOCAL_SCRIPTS",
            "defaultValue": "/app/deployment-scripts/node-deployment"
        },
        {
            "name": "TEST_DIR",
            "defaultValue": "/app/deployment-scripts/test"
        },
        {
            "name": "LICENSE_KEY",
            "defaultValue": "5361813d9531e729e2ec70773b60897c5d787391eedbf6a3d99ee06f163c51b6aca2db372c50ca8f9ced354447795856402fce6a33ab56a865b8c9cb11e1f0d44d433b8c2083eb193d4cb9ff2f37b3fbb0aa816184496c1f84e59e80ab3ef61cb8e79fb1614ad76a2d29572c4d9132fec7c0acbe492a7bd47cd62b119af874be2024-03-30bAnyLog"
        },
        {
            "name": "NODE_TYPE",
            "defaultValue": "master"
        },
        {
            "name": "NODE_NAME",
            "defaultValue": "anylog-master"
        },
        {
            "name": "COMPANY_NAME",
            "defaultValue": "New Company"
        },
        {
            "name": "ANYLOG_SERVER_PORT",
            "defaultValue": "32048"
        },
        {
            "name": "ANYLOG_REST_PORT",
            "defaultValue": "32049"
        },
        {
            "name": "TCP_BIND",
            "defaultValue": "false"
        },
        {
            "name": "REST_BIND",
            "defaultValue": "false"
        },
        {
            "name": "DB_TYPE",
            "defaultValue": "sqlite"
        },
        {
            "name": "DB_USER",
            "defaultValue": ""
        },
        {
            "name": "DB_PASSWD",
            "defaultValue": ""
        },
        {
            "name": "DB_IP",
            "defaultValue": "127.0.0.1"
        },
        {
            "name": "DB_PORT",
            "defaultValue": "5432"
        },
        {
            "name": "LEDGER_CONN",
            "defaultValue": "127.0.0.1:32048"
        },
        {
            "name": "MONITOR_NODES",
            "defaultValue": "true"
        }
    ],
    "deployment": {
        "services": {
            "anylog-node": {
                "image": "anylogco/anylog-network:latest",
                "network": "host"
            }
        }
    }
}
